{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Acronym",
  "description": "An acronym for each team, acronyms can be share by different teams names.",
  "required": [
    "name"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the acronym."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The actual Acronym."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    } 
  }
}