{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User",
  "description": "The User.",
  "required": [
    "name",
    "match_date",
    "local_id",
    "visitor_id",
    "local_result",
    "visitor_result",
    "league_id",
    "user_id",
    "video_url"
  ],
  "additionalProperties": false,
  "properties": {
    "match_date":{
      "$id": "#/properties/match_date",
      "type": "string",
      "title": "The date the match has been played."
    },
    "name":{
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of the vod."
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the vod."
    },
    "local_id": {
      "$id": "#/properties/local_id",
      "type": "integer",
      "title": "The id of the local team."
    },
    "visitor_id": {
      "$id": "#/properties/visitor_id",
      "type": "integer",
      "title": "The id of the visitant team."
    },
    "local_result": {
      "$id": "#/properties/local_result",
      "type": "integer",
      "title": "The local result."
    },
    "visitor_result": {
      "$id": "#/properties/visitor_result",
      "type": "integer",
      "title": "The visitor result."
    },
    "league_id": {
      "$id": "#/properties/league_id",
      "type": "integer",
      "title": "A id of the league."
    },
    "user_id": {
      "$id": "#/properties/user_id",
      "type": "integer",
      "title": "A id of the user that created the match."
    },
    "video_url": {
      "$id": "#/properties/video_url",
      "type": "string",
      "title": "The url of the vod, regularly wil be a link to youtube."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    } 
  }
}