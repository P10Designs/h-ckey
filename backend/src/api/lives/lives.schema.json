{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Logos",
  "description": "The logos of each team. ",
  "required": [
    "stream_url",
    "match_id"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the logo."
    },
    "stream_url": {
      "$id": "#/properties/stream_url",
      "type": "string",
      "title": "The url of the stream."
    },
    "match_id": {
      "$id": "#/properties/match_id",
      "type": "integer",
      "title": "The match_id of the stream."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    } 
  }
}