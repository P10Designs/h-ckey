{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Teams",
  "description": "The team.",
  "required": [
    "name",
    "acronym_id",
    "logo_id"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the team."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of the team .",
      "examples": [
        "Tres Cantos Pc"
      ]
    },
    "acronym_id": {
      "$id": "#/properties/acronym_id",
      "type": "integer",
      "title": "The id of the acronym of the team that it´s stored on a different table.",
      "examples": [
        "1"
      ]
    },
    "logo_id": {
      "$id": "#/properties/logo_id",
      "type": "integer",
      "title": "The id of the logo of the team that it´s stored on a different table.",
      "examples": [
        "1"
      ]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    } 
  }
}