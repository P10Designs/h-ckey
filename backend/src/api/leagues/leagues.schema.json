{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Leagues",
  "description": "The Leagues.",
  "required": [
    "name"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the league."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of the league."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the league on the database."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the league was last updated."
    }
  }
}