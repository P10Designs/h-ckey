{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "News",
  "description": "The News.",
  "required": [
    "name",
    "image_url",
    "new_url",
    "new_type_id",
    "league_id",
    "user_id"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the new."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of the new."
    },
    "image_url": {
      "$id": "#/properties/image_url",
      "type": "string",
      "title": "The url of the image miniature."
    },
    "new_url": {
      "$id": "#/properties/new_url",
      "type": "string",
      "title": "The url of the new page."
    },
    "new_type_id": {
      "$id": "#/properties/new_type_id",
      "type": "integer",
      "title": "The id of the type of new."
    },
    "league_id": {
      "$id": "#/properties/league_id",
      "type": "integer",
      "title": "The id of the league the new is refers to."
    },
    "user_id": {
      "$id": "#/properties/user_id",
      "type": "integer",
      "title": "The id of the user that created the new.",
      "examples": [
        "1"
      ]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    } 
  }
}