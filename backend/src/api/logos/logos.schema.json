{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Logos",
  "description": "The logos of each team. ",
  "required": [
    "url"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the logo."
    },
    "url": {
      "$id": "#/properties/url",
      "type": "string",
      "title": "The url of the logo on the cloud."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    } 
  }
}