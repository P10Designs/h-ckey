{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Matches",
  "description": "The Match.",
  "required": [
    "local_id",
    "visitor_id",
    "played",
    "match_time",
    "league_id",
    "user_id"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the match."
    },
    "local_id": {
      "$id": "#/properties/local_id",
      "type": "integer",
      "title": "The id of the local team."
    },
    "visitor_id": {
      "$id": "#/properties/visitor_id",
      "type": "integer",
      "title": "The id of the visitor team."
    },
    "played": {
      "$id": "#/properties/played",
      "type": "boolean",
      "title": "If match has been played or not."
    },
    "match_time": {
      "$id": "#/properties/match_time",
      "type": "string",
      "title": "The match date."
    },
    "league_id": {
      "$id": "#/properties/league_id",
      "type": "integer",
      "title": "The id of the league."
    },
    "user_id": {
      "$id": "#/properties/user_id",
      "type": "integer",
      "title": "The id of the user that created the match."
    },
    "vod_id": {
      "$id": "#/properties/vod_id",
      "type": "string",
      "title": "The id of the vod, if exists."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    } 
  }
}